<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sinapsis.inrecovery">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Bienvenida">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Seleccion" />
        <activity android:name=".Diagnostico" />
        <activity android:name=".ResultadoPositivo" />
        <activity android:name=".ResultadoNegativo" />
        <activity android:name=".MenuHerramientas" />
        <activity android:name=".Configuracion" />
        <activity android:name=".Historial" />
        <activity android:name=".Puntaje" />
        <activity android:name=".Herramientas" />
        <activity android:name=".ResumenHerramientas" />
        <activity android:name=".Glosario" />
        <activity android:name=".DescripcionHerramienta" />
        <activity android:name=".Feedback"></activity>
    </application>

</manifest>